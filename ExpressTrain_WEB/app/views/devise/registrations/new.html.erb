<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>
 <div class="container7"> 
  <h2>회원가입</h2>

  <div class="bd1">
    <div class="form-group">
      <label for="exampleFormControlInput1">ID</label>
      <input type="text" class="form-control" id="idname" name="member[email]" placeholder="Id 입력">
    </div>
    <div class="form-group">
      <label for="exampleFormControlInput1">EMAIL</label>
      <input type="email" class="form-control" id="mail" name="member[mail]" placeholder="mail 입력">
    </div>
    <div class="form-group">
      <label for="exampleFormControlInput1">NAME</label>
      <input type="text" class="form-control" id="name" name="member[name]" placeholder="name 입력">
    </div>
    <div class="form-group">
      <label for="exampleFormControlSelect1">PASSWORD</label>
      <input type="password" class="form-control" id="Password" name="member[password]" placeholder="password 입력">
    </div>
    <div class="form-group">
      <label for="exampleFormControlSelect1">PASSWORD_CONFIRMATION</label>
      <input type="password" class="form-control" id="Password" name="member[password_confirmation]" placeholder="password 입력">
    </div>
    <div class="b1">
      <button type="submit" name="member[submit]" class="btn btn-outline-fill">Sign up</button>
    </div>
 </div>

</div>
<% end %>